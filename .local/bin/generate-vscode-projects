#!/usr/bin/env pwsh
Get-ChildItem ~/src/AccelerateLearning -Directory | ForEach-Object {
    [PSCustomObject]@{
        name     = $_.Name
        rootPath = "vscode-remote://wsl+ubuntu/home/rcannon/src/AccelerateLearning/$($_.Name)"
        paths    = @()
        tags     = @()
        enabled  = $true
    }
} | ConvertTo-Json | Set-Content /mnt/c/Users/rob/AppData/Roaming/Code/User/globalStorage/alefragnani.project-manager/projects.json